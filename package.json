{
  "name": "mock-factory",
  "version": "0.0.1",
  "description": "Generate Mocks with random data",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint './src/**/*.ts'",
    "prettier": "prettier --write './src/**/*/.{ts,json,md}'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mock",
    "typescript"
  ],
  "author": "Nick Pisarski",
  "jest": {
    "reporters": [
      "default"
    ],
    "transform": {
      "\\.(js|ts)$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "./spec/.*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.2",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
